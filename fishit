        else
            game:GetService("Players").LocalPlayer.PlayerGui["Small Notification"].Display.Visible = true
        end
    end
})
local IsAnimOn = nil
local IsAnim = Main:Toggle({
    Title = "No Animation",
    Icon = "radar",
    Type = "Checkbox",
    Default = false,
    Callback = function(state)
        IsAnimOn = state 
        if IsAnimOn then
			anim = game:GetService("ReplicatedStorage").Modules.Animations.ReelingIdle
            animator:LoadAnimation(anim):Play()
        else
			anim = game:GetService("ReplicatedStorage").Modules.Animations.EquipIdle
            animator:LoadAnimation(anim):Play()
        end
    end
})

